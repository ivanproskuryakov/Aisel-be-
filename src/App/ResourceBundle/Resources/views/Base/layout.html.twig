{% extends 'MopaBootstrapBundle::base.html.twig' %}
{% block title %}{{ defaultMetaTitle() }}{% endblock title %}

{% block body_start %}
    {% include 'AppResourceBundle:Base:header.html.twig' %}
{% endblock body_start %}

{% block footer %}
    {% include 'AppResourceBundle:Base:footer.html.twig' %}
{% endblock footer %}

{% block foot_script %}
{% endblock foot_script %}

{% block head_style %}
    {% stylesheets
    '@AppResourceBundle/Resources/public/css/style.css'
    '@MopaBootstrapBundle/Resources/public/less/mopabootstrapbundle.less'
    filter='less' %}
    <link rel="stylesheet" type="text/css" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock head_style %}

{% block head_script %}
    {% javascripts
    'bower_components/jquery/jquery.min.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock head_script %}